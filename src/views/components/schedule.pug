section.schedule
  header
    h1 Schedule
    - var n = 9;
    ul
      while n < 17
        li
          if n > 12
            .time(class=n-12)= n - 12
              |  p.m.
            - n++
            each event in events
              include event-display.pug
          else if n === 12
            .time(class="12")= 12
              |  p.m.
            - n++
            each event in events
              include event-display.pug
          else
            .time(class=n)= n++
              |  a.m.
            each event in events
              include event-display.pug



